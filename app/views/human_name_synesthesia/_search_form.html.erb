
<%= search_form_for @q, url: url do |f| %>
    <div class="input-group mb-3">
      <%= f.search_field :name_cont, class: 'form-control', placeholder: t('defaults.search_word') %>
      <div class="input-group-append">
        <%= f.submit class: 'btn btn-dark' %>
      </div>
    </div>
  <% end %>