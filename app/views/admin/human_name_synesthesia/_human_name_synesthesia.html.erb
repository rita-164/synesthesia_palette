<tr>
  <td>
    <%= human_name_synesthesia.id %>
  </td>
  <td>
    <%= human_name_synesthesia.name %>
  </td>
  <td>
    <%= image_tag human_name_synesthesia.color_url, size: '300x50' %>
  </td>
  <td>
    <%= l human_name_synesthesia.created_at, format: :long %>
  </td>
  <td>
    <%= link_to t('defaults.show'), admin_human_name_synesthesium_path(human_name_synesthesia), class: 'btn btn-info' %>
    <%= link_to t('defaults.edit'), edit_admin_human_name_synesthesium_path(human_name_synesthesia), class: 'btn btn-success' %>
    <%= link_to t('defaults.delete'), admin_human_name_synesthesium_path(human_name_synesthesia), method: :delete, data: { confirm: t('defaults.message.delete_confirm') }, class: 'btn btn-danger' %>
  </td>
</tr>